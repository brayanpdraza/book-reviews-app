{
    "builds": [
      {
        "src": "frontend/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(ico|png|jpg|jpeg|svg|css|js|json))",
        "dest": "frontend/build/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/index.html"
      }
    ]
  }